                     <td width="300"><input type="checkbox"  style="zoom:2.5">Yes &emsp;<input type="checkbox"  style="zoom:2.5">No<br>
                                    <input type="checkbox"  style="zoom:2.5">Sometimes
             </tr>        
             
             
             
             
